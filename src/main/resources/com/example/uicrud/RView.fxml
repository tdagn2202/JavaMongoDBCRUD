<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="221.0" prefWidth="445.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.uicrud.RViewController">
   <children>
      <Label layoutX="113.0" layoutY="22.0" style="-fx-alignment: center;" text="RETRIVE STUDENT DATA" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button layoutX="72.0" layoutY="82.0" mnemonicParsing="false" onAction="#handleGetStudentById" prefHeight="25.0" prefWidth="153.0" text="Get student by id" textAlignment="CENTER" />
      <Button layoutX="72.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleGetStudentByName" prefHeight="25.0" prefWidth="153.0" text="Get student by name" textAlignment="CENTER" />
      <Button fx:id="btnGetAllStudent" layoutX="72.0" layoutY="146.0" mnemonicParsing="false" onAction="#handleGetAllStudent" prefHeight="25.0" prefWidth="315.0" text="Get all student" textAlignment="CENTER" />
      <Button layoutX="234.0" layoutY="82.0" mnemonicParsing="false" onAction="#handleGetStudentByAge" prefHeight="25.0" prefWidth="153.0" text="Get student by age" textAlignment="CENTER" />
      <Button layoutX="234.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleGetStudentByClassId" prefHeight="25.0" prefWidth="153.0" text="Get student by class id" textAlignment="CENTER" />
      <Button fx:id="backToMainFromRButton" layoutX="72.0" layoutY="176.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="25.0" prefWidth="315.0" text="Back to main" textAlignment="CENTER" />
   </children>
</AnchorPane>
